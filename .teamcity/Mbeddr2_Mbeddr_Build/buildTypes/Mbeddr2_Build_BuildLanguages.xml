<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="37785bfe-16c9-4c59-b369-be2231cb77d5" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Build mbeddr</name>
  <description />
  <settings ref="RequiresMpsExtions">
    <options>
      <option name="artifactRules" value="%teamcity.build.checkoutDir%/artifacts/" />
      <option name="buildNumberPattern" value="%mbeddr.build-number%" />
    </options>
    <parameters>
      <param name="mbeddr-scripts.path-properties" value="-Dmbeddr.scripts.base-path=%teamcity.build.checkoutDir%/scripts -Dplugins.scripts.base-path=%teamcity.build.checkoutDir%/scripts -Dsl-all.scripts.base-path=%teamcity.build.checkoutDir%/scripts" />
    </parameters>
    <build-runners order="RUNNER_87">
      <runner id="RUNNER_87" name="" type="Ant">
        <parameters>
          <param name="build-file-path" value="mbeddr.core/code/languages/build.xml" />
          <param name="jvmArgs" value="-XX:MaxPermSize=512m -Xmx4096m" />
          <param name="runnerArgs" value="%mps.path-properties% %artifacts-root.path-properties% %mbeddr-github.path-properties%  %build-dir.path-properties% %mbeddr-scripts.path-properties%" />
          <param name="target" value="build-platform-and-mbeddr" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Mbeddr2_Bootstrap_MbeddrBoostrap">
        <checkout-rule rule="%mbeddr.checkout-rules%" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency sourceBuildTypeId="Mbeddr2_Build_BuildSlAll" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="%generic.artifact-rule%" />
      </dependency>
      <dependency sourceBuildTypeId="Mbeddr2_Bootstrap_DownloadMps" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="%mps.artifact-rule%" />
      </dependency>
      <dependency sourceBuildTypeId="Mbeddr2_Bootstrap_GenerateBuildScripts" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="%buildscripts.artifact-rule%" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Mbeddr2_Build_BuildSlAll">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="run-build-if-dependency-failed-to-start" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Mbeddr2_Bootstrap_GenerateBuildScripts">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="run-build-if-dependency-failed-to-start" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Mbeddr2_Bootstrap_DownloadMps">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="run-build-if-dependency-failed-to-start" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

